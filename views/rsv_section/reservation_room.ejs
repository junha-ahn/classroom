<div class="btn-group btn-group-justified" role="group">
  <div class="btn-group" role="group">
    <button type="button" :class="classObjectMenu(1)" @click="chageMenuStep(1)">1. 강의실 선택</button>
  </div>
  <div class="btn-group" role="group">
    <button type="button" :class="classObjectMenu(2)" @click="chageMenuStep(2)">2. 날짜 선택</button>
  </div>
  <div class="btn-group" role="group">
    <button type="button" :class="classObjectMenu(3)" @click="chageMenuStep(3)">3. 시간 선택</button>
  </div>
  <div class="btn-group" role="group">
    <button type="button" :class="classObjectMenu(4)" @click="chageMenuStep(4)">4. 상세 내용 입력</button>
  </div>
  <div class="btn-group" role="group">
    <button type="button" :class="classObjectMenu(5)" @click="chageMenuStep(5)">5. 예약 내용 확인</button>
  </div>
</div>

<div>
  <button type="button" class="btn btn-default">교실</button>
  <button type="button" class="btn">{{section.room.room_number}} {{section.room.room_name}}</button>
  <br />
  <button type="button" class="btn btn-default">날짜</button>
  <button type="button" class="btn">{{section.room.date}}</button>
  <br />
  <button type="button" class="btn btn-default">시간</button>
  <button type="button"
    class="btn">{{(section.room.start_time && section.room.end_time) ? section.room.start_time + '~' + section.room.end_time : ''}}</button>
</div>
<div>
  <template v-if="section.room.menu_step==1">
    <dl>
      <dt v-for="(floor, i) in rooms" :key="i">
        <div class="btn-group-vertical" role="group" aria-label="...">
          <button type="button" class="btn btn-primary" @click="changeRoomFloor(i)">{{i}} 층</button>
          <dl>
            <dt v-for="(room, i) in floor" :key="i">
              <div v-if="room.floor==section.room.floor">
                <button type="button" class="btn btn-default" @click="chooseRoom(room)">{{room.room_number}},
                  {{room.name}} ({{room.seat_count}}석)</button>
              </div>
            </dt>
          </dl>
        </div>
      </dt>
    </dl>
  </template>
  <template v-else-if="section.room.menu_step==2">
    <calendar v-bind:holiday_results="holiday_results"></calendar>
    <% include ../component/calendar %>
  </template>
</div>